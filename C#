
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
 
namespace StructApp
{
    class Student : Person
    {
        public void Delaet()
        {
            Console.WriteLine("Persona jdet...");
        }
    }
    class Prepod
    {
        public string rabotaet;
        public string gde;
        public void Delaet()
        {
            Console.WriteLine("Prepod rabotaet v " + gde);
        }
    }
    class Person
    {
        public string chelovek;
        public string name;
        public void Delaet()
        {
            Console.WriteLine("chelovek " + name);
        }
    }
    class Zavkaf : Prepod
    {
        public void Delaet()
        {
            Console.WriteLine("Zavkaf rabotaet v " + gde);
        }
    }
    class Program
    {
        static void Main(string[] args)
        {
            Student persona = new Student();
            persona.Delaet();
            Prepod prepod = new Prepod();
            prepod.gde = "school";
            prepod.Delaet();
            Person person = new Person();
            person.name = "Artyom";
            person.Delaet();
            Zavkaf zavkaf = new Zavkaf();
            zavkaf.gde = "univere";
            zavkaf.Delaet();
 
            Console.ReadKey();
        }
    }
}
